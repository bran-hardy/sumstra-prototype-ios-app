{
  "expo": {
    "name": "sumstra-prototype",
    "slug": "sumstra-prototype",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sumstra-prototype-icon.png",
    "scheme": "sumstraprototype",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.thebranmuffin.sumstraprototype"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sumstra-prototype-logo@3x.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50356c0c-af9b-4e52-ac20-d7860edc0171"
      }
    },
    "owner": "thebranmuffin"
  }
}
